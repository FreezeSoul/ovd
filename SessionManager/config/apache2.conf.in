# Ulteo session manager alias

Alias /sessionmanager/base.tar.gz /var/cache/ulteo/ovd/sessionmanager/base.tar.gz

Alias /sessionmanager @DATADIR@/ulteo/sessionmanager
<Directory @DATADIR@/ulteo/sessionmanager>
    Options FollowSymLinks
    AllowOverride None
    Order allow,deny
    allow from all
    DirectoryIndex index.php
    php_admin_flag magic_quotes_gpc Off
</Directory>
