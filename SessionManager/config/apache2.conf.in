# Ulteo session manager alias

Alias /sessionmanager @DATADIR@/ulteo/sessionmanager
<Directory @DATADIR@/ulteo/sessionmanager>
    Options FollowSymLinks
    AllowOverride None
    Order allow,deny
    allow from all
    DirectoryIndex index.php
    php_admin_flag magic_quotes_gpc Off
</Directory>

NameVirtualHost *:1111
Listen 1111
<VirtualHost *:1111>
	DocumentRoot @DATADIR@/ulteo/sessionmanager/webservices
</VirtualHost>
